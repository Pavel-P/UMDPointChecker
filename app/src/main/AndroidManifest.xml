<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nat.pettipaw.com.pointchecker" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/NoTitle" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PointViewerActivity"
            android:label="@string/title_activity_point_viewer" >
        </activity>
        <activity
            android:name=".TransactionViewerActivity"
            android:label="@string/title_activity_transaction_viewer"
            android:theme="@style/NoTitle" >
        </activity>
    </application>

</manifest>
